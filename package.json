{
  "name": "kazagumo",
  "version": "3.4.0",
  "description": "A shoukaku wrapper with built-in queue support.",
  "main": "dist/Index.js",
  "types": "dist/Index.d.ts",
  "homepage": "https://github.com/Takiyo0/Kazagumo",
  "readme": "https://github.com/Takiyo0/Kazagumo#readme",
  "bugs": {
    "url": "https://github.com/Takiyo0/Kazagumo",
    "email": "takiyotakahashi@gmail.com"
  },
  "engines": {
    "node": ">=16.5.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:full": "npm run lint && npm run format && npm run build:docs && npm run build",
    "build": "tsc",
    "format": "prettier --write \"src/**/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build:docs": "typedoc --readme README.md --out docs/ --entryPointStrategy expand src/."
  },
  "keywords": [
    "lavalink",
    "lavalink-client",
    "shoukaku",
    "kazagumo",
    "lavalink-player",
    "player",
    "discord-player"
  ],
  "author": "Takiyo",
  "files": [
    "dist/**/**/*"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/node": "^24.10.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "discord.js": "^14.24.2",
    "events": "^3.3.0",
    "kazagumo-spotify": "file:../kazagumo-spotify",
    "prettier": "^3.6.2",
    "rebuild": "^0.1.2",
    "reinstall": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "shoukaku": "^4.2.0"
  }
}
